                    android:layout_alignLeft="@id/today_music_album_img1_iv"
                    android:layout_marginTop="180dp"
                    android:text="LILAC"
                    android:textColor="@color/black"
                    android:textSize="25sp" />

                <TextView
                    android:id="@+id/today_music_album_title2_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@id/today_music_album_img2_iv"
                    android:layout_alignLeft="@id/today_music_album_img2_iv"
                    android:layout_marginTop="180dp"
                    android:text="LILAC"
                    android:textColor="@color/black"
                    android:textSize="25sp" />

                <TextView
                    android:id="@+id/today_music_album_title3_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@id/today_music_album_img3_iv"
                    android:layout_alignLeft="@id/today_music_album_img3_iv"
                    android:layout_marginTop="180dp"
                    android:text="LILAC"
                    android:textColor="@color/black"
                    android:textSize="25sp" />

                <TextView
                    android:id="@+id/today_music_album_singer1_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="아이유 (IU)"
                    android:textColor="@color/grey"
                    android:textSize="18sp"
                    android:layout_alignTop="@id/today_music_album_img1_iv"
                    android:layout_alignLeft="@id/today_music_album_img1_iv"
                    android:layout_marginTop="215dp"/>

                <TextView
                    android:id="@+id/today_music_album_singer2_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="아이유 (IU)"
                    android:textColor="@color/grey"
                    android:textSize="18sp"
                    android:layout_alignTop="@id/today_music_album_img2_iv"
                    android:layout_alignLeft="@id/today_music_album_img2_iv"
                    android:layout_marginTop="215dp"/>

                <TextView
                    android:id="@+id/today_music_album_singer3_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="아이유 (IU)"
                    android:textColor="@color/grey"
                    android:textSize="18sp"
                    android:layout_alignTop="@id/today_music_album_img3_iv"
                    android:layout_alignLeft="@id/today_music_album_img3_iv"
                    android:layout_marginTop="215dp"/>

            </RelativeLayout>
        </HorizontalScrollView>

        <ImageView
