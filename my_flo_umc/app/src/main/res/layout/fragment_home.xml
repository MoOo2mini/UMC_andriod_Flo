                <ImageView
                    android:id="@+id/video_collection_img3_iv"
                    android:layout_width="300dp"
                    android:layout_height="170dp"
                    android:layout_marginStart="20dp"
                    android:layout_marginBottom="30dp"
                    android:src="@drawable/img_video_exp"
