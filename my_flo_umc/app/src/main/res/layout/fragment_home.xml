                    android:textSize="18sp"
                    android:layout_alignTop="@id/always_potcast_img3_iv"
                    android:layout_alignLeft="@id/always_potcast_img3_iv"
                    android:layout_marginTop="215dp"/>

            </RelativeLayout>
        </HorizontalScrollView>

        <TextView
